<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='SwissMuseumParcours'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  
  <!-- Inherit the mobile libraries.                       		  -->
  <inherits name='com.googlecode.gwtphonegap.PhoneGap' />
  <inherits name="com.googlecode.mgwt.MGWT"/>
  
  <!-- Inherits the project's core                                -->
  <inherits name='ch.sebastienzurfluh.swissmuseum.core.swissmuseum_core'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='ch.sebastienzurfluh.swissmuseum.parcours.client.SwissMuseumParcours'/>
  
  <!-- Set configurations 										  -->
  <set-configuration-property name="mgwt.css" value="pretty" />

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
</module>