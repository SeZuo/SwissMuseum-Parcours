<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='SwissMuseumParcours'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  
  <!-- Inherit the mobile libraries.                       		  -->
  <inherits name='com.googlecode.gwtphonegap.PhoneGap' />
  <inherits name="com.googlecode.mgwt.MGWT"/>
  
  <!-- App is for now only android/iphone compatible so we only need to compile for those devices -->
  <set-property name="user.agent" value="safari" />
  
  
  <inherits name='ch.sebastienzurfluh.swissmuseum.core.swissmuseum_core'/>
  
  
  
  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='ch.sebastienzurfluh.swissmuseum.parcours.client.SwissMuseumParcours'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
</module>